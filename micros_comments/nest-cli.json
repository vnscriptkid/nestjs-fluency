{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/micros_comments/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/micros_comments/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/micros_comments",
  "projects": {
    "micros_comments": {
      "type": "application",
      "root": "apps/micros_comments",
      "entryFile": "main",
      "sourceRoot": "apps/micros_comments/src",
      "compilerOptions": {
        "tsConfigPath": "apps/micros_comments/tsconfig.app.json"
      }
    },
    "comments-svc": {
      "type": "application",
      "root": "apps/comments-svc",
      "entryFile": "main",
      "sourceRoot": "apps/comments-svc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/comments-svc/tsconfig.app.json"
      }
    },
    "moderation-svc": {
      "type": "application",
      "root": "apps/moderation-svc",
      "entryFile": "main",
      "sourceRoot": "apps/moderation-svc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/moderation-svc/tsconfig.app.json"
      }
    },
    "notification-svc": {
      "type": "application",
      "root": "apps/notification-svc",
      "entryFile": "main",
      "sourceRoot": "apps/notification-svc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification-svc/tsconfig.app.json"
      }
    },
    "comment-svc": {
      "type": "application",
      "root": "apps/comment-svc",
      "entryFile": "main",
      "sourceRoot": "apps/comment-svc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/comment-svc/tsconfig.app.json"
      }
    }
  }
}